{
  "expo": {
    "name": "Scotteez Varsity Jacket",
    "slug": "Scotteez-Varsity-Jacket",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "icon": "./assets/SZicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashTest.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.mancusochr.scotteez"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ScotteezAdaptiveIcon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "dded6d67-7499-4756-8b66-f2d5450bde4f"
      }
    }
  }
}
